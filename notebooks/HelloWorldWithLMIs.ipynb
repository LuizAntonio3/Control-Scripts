   ],
   "source": [
    "x = cp.Variable(2)\n",
    "obj = cp.Minimize(x[0] + cp.norm(x, 1))\n",
    "constraints = [x >= 2]\n",
    "prob = cp.Problem(obj, constraints)\n",
    "\n",
    "prob.solve(solver=cp.MOSEK)\n",
    "print(\"optimal value with MOSEK:\", prob.value)"
   ]
  }
